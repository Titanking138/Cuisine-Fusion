   "source": [
    "import pandas as pd\n",
    "\n",
    "# Sample 3 rows per Region\n",
    "sampled_df = (\n",
    "    df.groupby('Region', group_keys=False)\n",
    "      .apply(lambda x: x.sample(n=3, random_state=42))\n",
    "      .reset_index(drop=True)\n",
    ")\n",
    "\n",
    "# Get unique cuisines from sampled data\n",
    "cuisines = sampled_df['Region'].unique()\n",
    "\n",
    "# Store transformed results\n",
    "transformed_data = []\n",
    "\n",
    "# Loop through each recipe in the sampled data\n",
    "for idx, row in sampled_df.iterrows():\n",
    "    source_cuisine = row['Region']\n",
    "    ingredients = row['cleaned_ingredients']\n",
    "    \n",
    "    # Transform to all other target cuisines\n",
    "    for target_cuisine in cuisines:\n",
    "        if target_cuisine == source_cuisine:\n",
    "            continue\n",
    "        \n",
    "        # Apply substitution\n",
    "        transformed_ingredients = [\n",
    "            get_most_suitable_substitute(ing, target_cuisine)\n",
    "            for ing in ingredients\n",
    "        ]\n",
    "        \n",
    "        # Store transformed result\n",
    "        transformed_data.append({\n",
    "            'recipe_id': row.get('Recipe_id', idx),  # fallback to index if Recipe_id not present\n",
    "            'source_cuisine': source_cuisine,\n",
    "            'target_cuisine': target_cuisine,\n",
    "            'original_ingredients': ingredients,\n",
    "            'transformed_ingredients': transformed_ingredients\n",
    "        })\n",
    "\n",
    "# Create the transformed dataframe\n",
    "transformed_df = pd.DataFrame(transformed_data)\n",
    "\n",
    "# Optional: Preview the results\n",
    "print(transformed_df.head())\n",
    "\n",
    "# Optional: Save for further analysis\n",
    "# transformed_df.to_csv('sampled_transformed_recipes.csv', index=False)\n"
   ]
