    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "Batches: 100%|| 366/366 [00:08<00:00, 44.01it/s]\n",
      "/tmp/ipykernel_1940760/377825027.py:51: DeprecationWarning: DataFrameGroupBy.apply operated on the grouping columns. This behavior is deprecated, and in a future version of pandas the grouping columns will be excluded from the operation. Either pass `include_groups=False` to exclude the groupings or explicitly select the grouping columns after groupby to silence this warning.\n",
      "  sampled_df = df.groupby('Sub_region', group_keys=False).apply(lambda x: x.sample(n=3, random_state=42)).reset_index(drop=True)\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Transformation complete!\n"
     ]
    }
   ],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "from collections import Counter\n",
    "from sentence_transformers import SentenceTransformer\n",
    "from sklearn.metrics.pairwise import cosine_similarity\n",
    "\n",
    "# Load dataset\n",
    "df = pd.read_csv(\"cleaned_recipes_db.csv\")\n",
    "df[\"cleaned_ingredients\"] = df[\"cleaned_ingredients\"].apply(eval)  # Convert string to list\n",
    "\n",
    "# Step 1: Extract top common ingredients per sub-region\n",
    "def get_common_ingredients(df, top_n=200):\n",
    "    cuisine_ingredients = {}\n",
    "    for cuisine, subset in df.groupby(\"Sub_region\"):\n",
    "        all_ingredients = [ing for sublist in subset[\"cleaned_ingredients\"] for ing in sublist]\n",
    "        common_ings = [ing for ing, _ in Counter(all_ingredients).most_common(top_n)]\n",
    "        cuisine_ingredients[cuisine.lower()] = set(common_ings)\n",
    "    return cuisine_ingredients\n",
    "\n",
    "cuisine_ingredients = get_common_ingredients(df)\n",
    "\n",
    "# Step 2: Compute sentence embeddings for each unique ingredient\n",
    "print(\"Encoding ingredient embeddings...\")\n",
    "bert_model = SentenceTransformer(\"all-MiniLM-L6-v2\")\n",
    "\n",
    "unique_ingredients = list({ing for sublist in df[\"cleaned_ingredients\"] for ing in sublist})\n",
    "ingredient_embeddings = bert_model.encode(unique_ingredients, show_progress_bar=True)\n",
    "ingredient_vectors = dict(zip(unique_ingredients, ingredient_embeddings))\n",
    "\n",
    "# Step 3: Define substitution logic\n",
    "def get_most_suitable_substitute(ingredient, target_cuisine, top_n=10):\n",
    "    if ingredient not in ingredient_vectors:\n",
    "        return ingredient\n",
    "\n",
    "    ingredient_vec = ingredient_vectors[ingredient].reshape(1, -1)\n",
    "    \n",
    "    # Compute similarity with all others\n",
    "    similarities = {\n",
    "        ing: cosine_similarity(ingredient_vec, vec.reshape(1, -1))[0][0]\n",
    "        for ing, vec in ingredient_vectors.items() if ing != ingredient\n",
    "    }\n",
    "\n",
    "    # Sort by similarity and filter by target cuisine\n",
    "    sorted_subs = sorted(similarities.items(), key=lambda x: x[1], reverse=True)\n",
    "    for ing, sim in sorted_subs:\n",
    "        if ing in cuisine_ingredients.get(target_cuisine.lower(), set()):\n",
    "            return ing\n",
    "    return ingredient  # If nothing suitable, return original\n",
    "\n",
    "# Step 4: Sample 3 recipes per sub-region to keep it fast\n",
    "sampled_df = df.groupby('Sub_region', group_keys=False).apply(lambda x: x.sample(n=3, random_state=42)).reset_index(drop=True)\n",
    "\n",
    "# Step 5: Generate all combinations: each recipe to every other cuisine\n",
    "results = []\n",
    "sub_regions = sampled_df[\"Sub_region\"].unique().tolist()\n",
    "\n",
    "for _, row in sampled_df.iterrows():\n",
    "    original_id = row[\"Recipe_id\"]\n",
    "    source_cuisine = row[\"Sub_region\"]\n",
    "    original_ingredients = row[\"cleaned_ingredients\"]\n",
    "\n",
    "    for target_cuisine in sub_regions:\n",
    "        transformed = [\n",
    "            get_most_suitable_substitute(ing, target_cuisine)\n",
    "            for ing in original_ingredients\n",
    "        ]\n",
    "        results.append({\n",
    "            \"recipe_id\": original_id,\n",
    "            \"source_cuisine\": source_cuisine,\n",
    "            \"target_cuisine\": target_cuisine,\n",
    "            \"original_ingredients\": original_ingredients,\n",
    "            \"transformed_ingredients\": transformed\n",
    "        })\n",
    "\n",
    "# Convert results to DataFrame\n",
    "final_df = pd.DataFrame(results)\n",
    "\n",
    "# Save to CSV (optional)\n",
    "final_df.to_csv(\"transformed_ingredients_sample.csv\", index=False)\n",
    "\n",
    "print(\"Transformation complete!\")\n"
   ]
  },
