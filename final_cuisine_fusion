  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "from collections import Counter\n",
    "\n",
    "# Load dataset\n",
    "df = pd.read_csv(\"cleaned_recipes_db.csv\")\n",
    "\n",
    "# Ensure ingredients are stored as lists\n",
    "df[\"cleaned_ingredients\"] = df[\"cleaned_ingredients\"].apply(eval)  # Convert from string to list\n",
    "\n",
    "# Step 1: Extract common ingredients for each cuisine\n",
    "def get_common_ingredients(df, top_n=50):\n",
    "    \"\"\"Extracts the most common ingredients for each cuisine.\"\"\"\n",
    "    cuisine_ingredients = {}\n",
    "\n",
    "    for cuisine, subset in df.groupby(\"Sub_region\"):  # Change to 'Cuisine' if needed\n",
    "        all_ingredients = [ing for sublist in subset[\"cleaned_ingredients\"] for ing in sublist]  # Flatten list\n",
    "        common_ings = [ing for ing, _ in Counter(all_ingredients).most_common(top_n)]  # Get top ingredients\n",
    "        \n",
    "        cuisine_ingredients[cuisine.lower()] = set(common_ings)  # Store in lowercase for consistency\n",
    "\n",
    "    return cuisine_ingredients\n",
    "\n",
    "# Generate cuisine-specific ingredient lists\n",
    "cuisine_ingredients = get_common_ingredients(df, top_n=100)\n",
    "\n",
    "\n",
    "def get_most_suitable_substitute(ingredient, target_cuisine, top_n=10):\n",
    "    \"\"\"Find the best ingredient substitute that belongs to the target cuisine.\"\"\"\n",
    "    target_cuisine = target_cuisine.lower()\n",
    "    if ingredient not in ingredient_vectors:\n",
    "        return ingredient  # If unknown, keep original\n",
    "    \n",
    "    # Compute cosine similarity with all known ingredients\n",
    "    similarities = {\n",
    "        other_ing: np.dot(ingredient_vectors[ingredient], ingredient_vectors[other_ing])\n",
    "        for other_ing in ingredient_vectors if other_ing != ingredient\n",
    "    }\n",
    "    \n",
    "    # Rank by similarity\n",
    "    sorted_substitutes = sorted(similarities, key=similarities.get, reverse=True)\n",
    "    \n",
    "    # Filter substitutes that belong to the target cuisine\n",
    "    suitable_substitutes = [ing for ing in sorted_substitutes if ing in cuisine_ingredients.get(target_cuisine, set())]\n",
    "\n",
    "    return suitable_substitutes[0] if suitable_substitutes else ingredient \n",
    "\n",
    "\n",
    "# Example: Convert an **Indian recipe** to **Italian cuisine**\n",
    "# target_cuisine = \"italian\"\n",
    "\n",
    "# df[\"transformed_ingredients\"] = df[\"cleaned_ingredients\"].apply(\n",
