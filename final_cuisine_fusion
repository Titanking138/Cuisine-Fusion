    "from collections import defaultdict\n",
    "\n",
    "# Create automatic cuisine-based ingredient mapping\n",
    "cuisine_based_substitutions = defaultdict(dict)\n",
    "\n",
    "# Generate automatic substitutions per cuisine\n",
    "for cuisine in df['Sub_region'].unique():\n",
    "    subset = df[df['Sub_region'] == cuisine]\n",
    "    \n",
    "    # Collect unique ingredients for this cuisine\n",
    "    unique_ingredients = set([ing for sublist in subset['cleaned_ingredients'] for ing in sublist])\n",
    "\n",
    "    # Find similar substitutes for each ingredient\n",
    "    for ingredient in unique_ingredients:\n",
    "        similar_ings = get_similar_ingredients(ingredient, top_n=3)  # Top 3 substitutes\n",
    "        if similar_ings:\n",
    "            cuisine_based_substitutions[cuisine][ingredient] = similar_ings\n",
    "\n",
    "# Print generated substitutions for a specific cuisine\n",
    "print(\"Generated substitutions for Italian cuisine:\", cuisine_based_substitutions[\"italian\"])\n"
   ]
