  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "import ast\n",
    "import re\n",
    "\n",
    "# Load dataset (assuming a CSV file)\n",
    "df = pd.read_csv(\"cleaned_recipes_db.csv\")  \n",
    "\n",
    "def clean_ingredient(ingredient):\n",
    "    \"\"\"Normalize ingredient names (lowercase, remove extra spaces, unify formats).\"\"\"\n",
    "    ingredient = ingredient.lower().strip()\n",
    "    ingredient = re.sub(r'\\s+', ' ', ingredient)  # Remove extra spaces\n",
    "    return ingredient\n",
    "\n",
    "# Ensure cleaned_ingredients is a proper list\n",
    "df['cleaned_ingredients'] = df['cleaned_ingredients'].apply(\n",
    "    lambda x: ast.literal_eval(x) if isinstance(x, str) else x\n",
    ")\n",
    "\n",
    "# Normalize ingredient names\n",
    "df['cleaned_ingredients'] = df['cleaned_ingredients'].apply(\n",
    "    lambda x: [clean_ingredient(i) for i in x] if isinstance(x, list) else x\n",
    ")\n",
    "\n",
    "# Extract cuisine labels if available\n",
    "if \"Sub_region\" in df.columns:\n",
    "    df['Sub_region'] = df['Sub_region'].str.lower().str.strip()\n",
    "else:\n",
    "    print(\"Cuisine column not found! Ensure dataset has cuisine labels.\")\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Substitutes for cumin: ['cumin seed heat', 'cumin powder', 'cumin seed', 'mccormick hot mexican chili powder', 'paprika chili powder']\n"
     ]
    }
   ],
   "source": [
    "from gensim.models import FastText\n",
    "\n",
    "# Create ingredient sentences (each recipe as a \"sentence\" of ingredients)\n",
    "sentences = df['cleaned_ingredients'].dropna().tolist()\n",
    "\n",
    "# Train FastText model\n",
    "fasttext_model = FastText(sentences, vector_size=100, window=5, min_count=1, workers=4, sg=1)\n",
    "\n",
    "def get_similar_ingredients(ingredient, top_n=5):\n",
    "    \"\"\"Find similar ingredients using FastText embeddings.\"\"\"\n",
    "    try:\n",
    "        return [i[0] for i in fasttext_model.wv.most_similar(ingredient, topn=top_n)]\n",
    "    except KeyError:\n",
    "        return []  # If ingredient not found in vocabulary\n",
    "\n",
    "# Example: Get substitutes for cumin\n",
    "print(\"Substitutes for cumin:\", get_similar_ingredients(\"cumin\"))\n"
   ]
  },
